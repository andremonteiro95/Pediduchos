<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pt.ubi.di.pdm.pediduchos">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PediduchosMain"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PedidoCriar"
            android:screenOrientation="portrait"/>
        <activity android:name=".PedidoDetalhes"
            android:screenOrientation="portrait"/>
        <activity android:name=".PediduchosLogin"
            android:screenOrientation="portrait"/>
        <activity android:name=".PediduchosAdmin"
            android:screenOrientation="portrait"/>
        <activity android:name=".ProdutoCriar"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
